SELECT *
FROM CUSTOMERS 

SELECT *
FROM JanSales 

SELECT *
FROM CUSTOMERS 
WHERE length(order_id) = 6
AND order_id <> 'Order ID'

SELECT COUNT(orderid)
FROM JanSales
WHERE length(orderid) = 6
AND orderid <> 'Order ID'

SELECT COUNT(Product)
FROM JanSales
WHERE length(orderid) = 6
AND orderid <> 'Order ID'
AND Product="iPhone"

SELECT DISTINCT acctnum
FROM customers
INNER JOIN FebSales 
ON customers.order_id= FebSales.orderID
WHERE length(orderid) = 6
AND orderid <> 'Order ID'

SELECT DISTINCT Product, MIN(price)
From JanSales
GROUP BY product

SELECT product, min(price)
FROM BIT_DB.JanSales Jan
GROUP BY product, price
ORDER BY price ASC
LIMIT 1;

SELECT product, SUM(price)
FROM JanSales
Group by product

SELECT sum(quantity)*price as revenue
,product
FROM BIT_DB.JanSales
GROUP BY product;

SELECT Product, location, Quantity, sum(Quantity)*price as Total_Rev
From FebSales
WHERE location ='548 Lincoln St, Seattle, WA 98101'

SELECT
sum(Quantity),product,sum(quantity)*price as revenue
FROM BIT_DB.FebSales
WHERE location = '548 Lincoln St, Seattle, WA 98101'
GROUP BY product;

SELECT COUNT(distinct customers.acctnum),AVG(quantity)*price, quantity, acctnum
FROM FebSales
LEFT JOIN customers
ON customers.order_id=FebSales.orderID
Where quantity>=2
GROUP By customers.acctnum

SELECT
count(distinct cust.acctnum),
avg(quantity*price)
FROM BIT_DB.FebSales Feb
LEFT JOIN BIT_DB.customers cust
ON FEB.orderid=cust.order_id
WHERE Feb.Quantity>2
AND length(orderid) = 6
AND orderid <> 'Order ID';